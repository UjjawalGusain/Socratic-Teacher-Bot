{
    "version": 2,
    "name": "socratic_teacher_bot",
    "builds": [
        {
            "src": "server.js",  
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"  
        }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/" }]
   
}
